{
  "name": "nwc-explorer",
  "version": "1.0.11",
  "description": "Experimental Visual Stidio Code extension to browse your Nintex Workflow Cloud tenantsm",
  "homepage": "",
  "license": "ISC",
  "main": "./dist/main.js",
  "private": true,
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NTX-XT/nwc-sdk.git"
  },
  "scripts": {
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=dist/main.js --external:vscode --format=cjs --platform=node",
    "build:dev": "npm run esbuild-base -- --sourcemap",
    "build": "npm run esbuild-base -- --sourcemap",
    "package": "vsce package --no-dependencies --out ./bin/",
    "postbuild": "npm run package",
    "publish": "vsce publish"
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "title": "NWC Explorer",
          "id": "nwc-explorer",
          "icon": "resources/nintex.png"
        }
      ]
    },
    "views": {
      "nwc-explorer": [
        {
          "id": "tenants",
          "name": "Tenants",
          "icon": "resources/nintex.png",
          "contextualTitle": "NWC Explorer"
        }
      ]
    },
    "commands": [
      {
        "command": "tenants.connect",
        "title": "NWC: Connect to tenant"
      },
      {
        "command": "tenants.refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "tenants.edit",
        "title": "Edit",
        "icon": {
          "light": "resources/light/edit.svg",
          "dark": "resources/dark/edit.svg"
        }
      },
      {
        "command": "tenants.viewData",
        "title": "View Data"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "tenants.refresh",
          "when": "view == tenants",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "tenants.viewData",
          "group": "navigation",
          "icon": {
            "light": "resources/light/edit.svg",
            "dark": "resources/dark/edit.svg"
          }
        }
      ]
    },
    "configuration": {
      "title": "NWC Explorer",
      "type": "object",
      "properties": {
        "nwcExplorer.connections": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "clientId": {
                "type": "string",
                "title": "Client Id"
              },
              "clientSecret": {
                "type": "string",
                "title": "Client Secret"
              }
            }
          }
        },
        "nwcExplorer.packageFilePath": {
          "type": [
            "string",
            null
          ],
          "default": null
        }
      }
    }
  },
  "dependencies": {
    "@nwc-sdk/client": "^1.0.15",
    "@vscode/codicons": "^0.0.27",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0"
  }
}
